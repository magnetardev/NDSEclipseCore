{
  "public": "docs/",
  "cleanUrls": false,
  "headers": [
    {
      "source": "**/*.@(html|js)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
}
